g(agent, message, ssml=False, emotion=None, enable_delay=True):\n    timestamp = datetime.now().isoformat()\n\n    # Logging\n    if ENABLE_LOGGING:\n        df = pd.DataFrame([[timestamp, agent, message]], columns=[\"Timestamp\", \"Agent\", \"Message\"])\n        df.to_csv(LOG_FILE, mode='a', index=False, header=False)\n\n    if ENABLE_TTS:\n        voice_name = AGENT_VOICES.get(agent, \"en-US-Wavenet-D\")\n        text_input = f\"<speak>{message}</speak>\" if ssml else message\n\n        synthesis_input = texttospeech.SynthesisInput(ssml=text_input) if ssml else texttospeech.SynthesisInput(text=message)\n\n        voice_params = texttospeech.VoiceSelectionParams(\n            language_code=voice_name[:5],\n            name=voice_name\n        )\n\n        audio_config = texttospeech.AudioConfig(\n            audio_encoding=texttospeech.AudioEncoding.MP3,\n            speaking_rate=0.95,\n            pitch=0.0\n        )\n\n        response = tts_client.synthesize_speech(\n            input=synthesis_input,\n            voice=voice_params,\n            audio_config=audio_config\n        )\n\n        filename = f\"{AUDIO_DIR}/tts_{agent}_{int(time.time())}.mp3\"\n        with open(filename, \"wb\") as out:\n            out.write(response.audio_content)\n        os.system(f\"mpg123 {filename}\")\n\n    if enable_delay:\n        time.sleep(random.uniform(0.5, 1.5))\n\n# Agent dialogue with sound FX and emotion\ndef client_interaction_agent(state):\n    play_sound_effect(\"ringing\")\n    enhanced_speak_and_log(\"ClientAgent\", \"<speak><prosody rate='fast'>ğŸ“ Hello? I fell down... I'm alone... my leg hurts badly!</prosody></speak>\", ssml=True)\n    enhanced_speak_and_log(\"ClientInteractionAgent\", f\"<speak>Hi {state['name']}, we're here to help. You're in {state['location']}, right?</speak>\", ssml=True)\n    enhanced_speak_and_log(\"ClientAgent\", \"<speak>Yes, and... and I think I broke my leg. I also have a pacemaker!</speak>\", ssml=True)\n    enhanced_speak_a